#include <stdio.h>
#include <stdbool.h>

bool isPrime(int n)
{
    int i, c = 0;
    for (i = 1; i <= n; i++)
    {
        if (n % i == 0)
            c++;
    }
    if(c == 2) return true;
    else return false;
}

int main()
{
    int s, e, i, cp = 0, alt = 0;
    printf("Enter the value of s and e: ");
    scanf("%d %d", &s, &e);
    for (i = s; i <= e; i++)
    {
        if (isPrime(i))
        {
            if (alt % 2 == 0) 
            {
                printf("%d ", i);
                cp++;
            }
            alt++;
        }
    }

    printf("\nNumber of alternate prime numbers is %d\n", cp);
    return 0;
}

